{
    "name": "label84/laravel-logviewer",
    "description": "A log viewer package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@tjardo.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },    
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "league/factory-muffin": "^3.3",
        "league/factory-muffin-faker": "^2.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\LogViewer\\LogViewerServiceProvider"
            ],
            "aliases": {
                "LogViewer": "Label84\\LogViewer\\Facades\\LogViewer"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Label84\\LogViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\LogViewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
