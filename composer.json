{
    "name": "label84/laravel-logviewer",
    "description": "A log viewer package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@label84.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/auth": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0"
    },    
    "autoload": {
        "psr-4": {
            "Label84\\LogViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\LogViewer\\Tests\\": "tests"
        }
    },    
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\LogViewer\\LogViewerServiceProvider"
            ],
            "aliases": {
                "LogViewer": "Label84\\LogViewer\\Facades\\LogViewer"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nunomaduro/larastan": "^0.6.11",
        "phpunit/phpunit": "^9.5",
        "league/factory-muffin": "^3.3",
        "league/factory-muffin-faker": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
